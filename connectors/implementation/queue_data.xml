<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record forcecreate="True" id="ir_cron_faux_queue_magento_default" model="ir.cron">
            <field name="name">Magento Tasks</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="'faux.queue.magento'" name="model"/>
            <field eval="'run'" name="function"/>
            <field eval="'(\'default\',)'" name="args"/>
        </record>

        <!-- second queue for the orders to increase their priority -->
        <record forcecreate="True" id="ir_cron_faux_queue_magento_orders" model="ir.cron">
            <field name="name">Import Magento Orders</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field eval="'faux.queue.magento'" name="model"/>
            <field eval="'run'" name="function"/>
            <field eval="'(\'orders\',)'" name="args"/>
        </record>
    </data>
</openerp>
